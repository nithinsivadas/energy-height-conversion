% Error slices!

alpha_0 = [5000,10000,10,1000];

x_0 = 100:100:20000;

y_0 = kappa_j(alpha_0,x_0);

a1=0:100:10000;
a2=1000:1000:20000;
a3=1.5:1:15;
a4=100:100:2000;

% Residue
for i=1:1:length(a1)
    for j=1:1:length(a2)
        for k=1:1:length(a3)
            for m=1:1:length(a4)
                R(i,j,k,m) = sum((kappa_j([a1(i),a2(j),a3(k),a4(m)],x_0)-y_0).^2);
            end
        end
    end
end

figure;
